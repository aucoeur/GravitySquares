  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "Aucoeur Ngo",
    "email": "aucoeurngo@gmail.com",
    "url": "https://aucoeur.dev"
  },
  "build": {
    "appId": "dev.aucoeur.tetris",
    "productName": "Tetris",
    "copyright": "Tetris belongs to Nintendo not me rip",
    "mac": {
